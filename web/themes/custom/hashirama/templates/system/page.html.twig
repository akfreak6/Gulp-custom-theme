
<div class="layout-container">

  <header role="banner">
    {{ page.header }}
  </header>

  {{ page.primary_menu }}

{% if page.banner_top %}"
<div class="layout-banner_top" role="banner">
{{ page.banner_top }}
{%endif%}
</div>

{% if page.banner_bottom %}"
<div class="layout-banner_bottom" role="banner">
{{ page.banner_bottom }}
{%endif%}
</div>

  <main role="main">
    <a id="main-content" tabindex="-1"></a>{# link is in html.html.twig #}

    <div class="layout-content">
      {{ page.content }}

    {{ page.content_bottom}}
    
    </div>{# /.layout-content #}


  </main>

  {% if page.footer %}
    <footer role="contentinfo">
      {{ page.footer }}
    </footer>
  {% endif %}

</div>{# /.layout-container #}
